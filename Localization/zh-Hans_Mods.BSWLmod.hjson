Configs: {
	Config: {
		DisplayName: 打怪升级模组设置

		Headers: {
			HeaderDMG: "[i:674] 伤害加成系数 [i:674]"
			HeaderXP: "[i:BSWLmod/PrismRune] 经验获取系数 [i:BSWLmod/PrismRune]"
			HeaderCaps: "[i:BSWLmod/OverclockedRune] 数值上限 [i:BSWLmod/OverclockedRune]"
			HeaderText: "[i:171] 信息显示 [i:171]"
			HeaderEtc: "[i:BSWLmod/ShinyDormant] 额外功能 [i:BSWLmod/ShinyDormant]"
		}

		dmgMultiplier: {
			Label: "[i:BSWLmod/UnstableRune] 武器伤害加成系数"
			Tooltip:
				'''
				调整武器等级的伤害提升效果
				默认状态下，武器在 100 级时最高有 3 倍伤害加成
				此系数参与以下计算公式：武器当前级别的伤害加成倍率 = ((基础伤害加成-1)*此系数)+1
				将此项数值调低将减少武器等级的伤害加成，调高将增加伤害加成
				将此项数值调为 0 将完全禁用武器等级伤害加成
				'''
		}

		playerDamage: {
			Label: "[i:2349] 玩家伤害加成系数"
			Tooltip:
				'''
				调整玩家等级的伤害提升效果
				默认状态下，玩家在 100 级时最高有 1.4 倍伤害加成，从 10 级开始计算
				此系数参与以下计算公式：玩家当前级别的伤害加成倍率 = ((基础伤害加成-1)*此系数)+1
				将此项数值调低将减少玩家等级的伤害加成，调高将增加伤害加成
				将此项数值调为 0 将完全禁用玩家等级伤害加成
				'''
		}

		xpGain: {
			Label: "[i:BSWLmod/BasicRune] 武器经验获取"
			Tooltip:
				'''
				切换武器是否可以通过造成伤害获取经验值
				武器对大多数敌人造成的伤害后将得到经验值，累计经验可升级
				请注意，在开启选项后，你还需要把经验符文镶嵌在对应武器上来启用其经验获取
				关闭后，所有武器都将停止获取经验值，直到重新开启此选项
				'''
		}

		playerXP: {
			Label: "[i:267] 玩家经验获取"
			Tooltip:
				'''
				切换玩家是否可以通过造成伤害获取经验值
				与武器不同，玩家获取的经验值总是等于所造成的伤害，而玩家能否获取经验值也完全取决于这个设置选项
				此外，玩家等级的提升要难得多，达到 100 级所需的经验值远超玩家在正常游戏流程中能获得的平均值
				作为参考，在击败月亮领主后，预计玩家可以最终达到 45-50 级（280-510万经验值）左右
				升到 100 级需要高达100亿经验值，所以不要指望通过正常方式达到此目标...
				关闭后，玩家将停止获取经验值，直到重新开启此选项
				'''
		}

		xpMultiplier: {
			Label: "[i:BSWLmod/EnhancedRune] 经验获取系数"
			Tooltip:
