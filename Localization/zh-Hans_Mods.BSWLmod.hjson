		orbMultiplier: {
			Label: "[i:BSWLmod/SuperchargedXP] 宝珠效果系数"
			Tooltip:
				'''
				调整宝珠对武器经验值给与/扣除效果
				沉寂宝珠是敌怪概率掉落的稀有物品，可以制作成经验宝珠/勾销宝珠，给与/扣除武器的经验值
				作为参考，默认状态下，从 0 级到 100 级需要大约60个最高等级的经验宝珠或6000个最弱等级的经验宝珠
				调整滑块以改变宝珠对经验给与/扣除的量：数值越高，效果越大；数值越低，效果越小
				请注意，此选项不会影响奥妙经验宝珠/动荡勾销宝珠的效果，它们直接改变武器的等级
				'''
		}

		effortMultiplier: {
			Label: "[i:BSWLmod/EffortRune] 效率符文加成系数"
			Tooltip:
